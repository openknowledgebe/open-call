{
    "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["svelte3", "prettier"],
    "ignorePatterns": ["node_modules/"],
    "overrides": [
      {
        "files": ["**/*.svelte"],
        "processor": "svelte3/svelte3"
      }
    ],
    "globals": {
      "process": true
    }
  }
  